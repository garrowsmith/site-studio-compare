{
    "name": "garrowsmith/site-studio-compare",
    "description": "Site Studio package comparison tool.",
    "type": "project",
    "require": {
        "php": "^7.3",
        "swaggest/json-diff": "^3.8",
        "symfony/yaml": "^5.2"
    },
    "autoload": {
        "psr-4": {
          "SiteStudio\\Package\\": "src/Package"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "SiteStudio\\Compare\\Tests\\": "tests/src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
